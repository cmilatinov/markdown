{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*.ts",
        "src/**/*.vue"
    ],
    "exclude": [
        "src/**/*.spec.ts"
    ],
    "compilerOptions": {
        "composite": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}
